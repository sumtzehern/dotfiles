# Save a lot of typing for common commands
alias gs="git status"
alias gc="git commit"
alias v="vim"
alias cd..="cd .."
alias "git add."="git add ."

# Save you from mistyping
alias sl=ls
source /Users/MacbookPro/.docker/init-bash.sh || true # Added by Docker Desktop

# Short cut for tmux config file
alias vtmux='cd ~ && tmux -f ~/.tmux.conf'


